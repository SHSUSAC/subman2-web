    "@emotion/core": "11.0.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@firebase/logger": "0.3.2",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@headlessui/react": "1.4.2",
    "@hookform/error-message": "2.0.0",
    "@js-temporal/polyfill": "0.3.0",
    "@next/bundle-analyzer": "12.0.7",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/node": "16.11.19",
    "@types/prettier": "2.4.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-table": "7.7.9",
    "@typescript-eslint/parser": "5.9.0",
    "autoprefixer": "10.4.1",
    "babel-plugin-macros": "3.1.0",
    "browser-bunyan": "1.6.3",
    "del": "6.0.0",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "fancy-log": "1.3.3",
    "firebase": "9.6.1",
    "firebase-tools": "10.0.1",
    "git-guppy": "2.1.0",
    "gulp": "4.0.2",
    "gulp-eslint": "6.0.0",
    "gulp-filter": "7.0.0",
    "gulp-real-favicon": "0.3.2",
    "guppy-pre-commit": "0.4.0",
    "next-optimized-images": "3.0.0-canary.10",
    "optimized-images-loader": "0.4.0",
    "plugin-error": "1.0.1",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "query-string": "7.0.1",
    "react-click-away-listener": "2.0.5",
    "react-hook-form": "7.22.5",
    "react-optimized-image": "0.4.1",
    "react-select": "5.2.1",
    "react-table": "7.7.0",
    "react-toastify": "8.1.0",
    "react-use-error-boundary": "0.0.6",
    "reactfire": "4.2.1",
    "rxfire": "6.0.3",
    "rxjs": "7.5.1",
    "safe-json-stringify": "1.2.0",
    "sass": "1.45.2",
    "semantic-release": "18.0.1",
    "tailwindcss": "2.2.19",
    "tailwindcss-animatecss": "1.0.7",
    "tailwindcss-multi-column": "1.0.2",
    "tailwindcss-question-mark": "0.1.1",
    "twin.macro": "2.8.2",
    "typescript": "4.5.4",
    "use-async-resource": "2.2.2",
    "use-local-storage-state": "13.0.0"
  },
  "dependencies": {
    "next": "12.0.7",
    "next-compose-plugins": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Semantic Versioning Changelog"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
			"yarn.lock"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "out/**",
              "name": "Static Build",
              "label": "Website, statically built for deployment on a HTTP server"
            }
          ]
        }
      ]
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js"
    }
  }
}
